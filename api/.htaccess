RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ index.php [QSA,L]

RedirectMatch 404 composer.json
RedirectMatch 404 composer.lock
RedirectMatch 404 db.schema.sql
RedirectMatch 404 Makefile
RedirectMatch 404 Procfile.dev
RedirectMatch 404 README.md
RedirectMatch 404 test
RedirectMatch 404 tests
RedirectMatch 404 test_environment.sql
RedirectMatch 404 vendor
RedirectMatch 404 .user.ini
